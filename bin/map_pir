#!/bin/bash

app=$1
log=$2

mkdir -p $PIR_HOME/out/$app
$PIR_HOME/bin/pir $app \
  --debug \
  --mapping=true \
  --net=static \
  --topo=mesh \
  --row=16 --col=8 \
  --load-pir=false \
  --save-pir=true \
  --psim=false \
  --ctrl=true \
  --bp=false \
  --stat=true > $log 2>&1 

echo "-------------PASS (DONE)------------" >> $log
