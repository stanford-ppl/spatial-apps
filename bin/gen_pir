#!/bin/bash

cd $SPATIAL_HOME
pir_dir=$SPATIAL_HOME/gen/$app/pir
app_file=$PIR_HOME/apps/src/gen/${app}.scala
if [ -e $pir_dir ]; then rm -r $pir_dir; fi
if [ -e $app_file ]; then rm $app_file; fi
mkdir -p $SPATIAL_HOME/gen/$app
mkdir -p $PIR_HOME/apps/src/gen/
mkdir -p $pir_dir
$SPATIAL_HOME/bin/spatial $app --dot --cgra+ &>> $log &
