#!/bin/bash

app=$1
log=$2

mkdir -p $PIR_HOME/out/$app
export _JAVA_OPTIONS="-Xss8192k" 
$PIR_HOME/bin/pir $app \
  --debug \
  --mapping=false \
  --load-pir=false \
  --save-pir=true \
  --psim=false \
  --ctrl=true \
  --bp=false \
  --stat=true \
  --arch=MyDesign \
  --net=dynamic \
  --topo=mesh \
  --row=2 --col=2 \
  --argin=64 \
  > $log 2>&1 

echo "-------------PASS (DONE)------------" >> $log
