#!/bin/bash

# declare an array called array and define 3 vales
files=( "DotProduct" "OuterProduct" )
#files=( "OuterProduct" )
for app in "${files[@]}"
do
  echo $app
  rm -r $SPATIAL_HOME/gen/$app
  $SPATIAL_HOME/bin/spatial $app --synth --naming 1

  cd $SPATIAL_HOME/gen/$app/
  make vcs
  source run.sh 6400 > sim.log
  grep "Design ran for" sim.log
  grep "PASS" sim.log
  cd ../../
done
